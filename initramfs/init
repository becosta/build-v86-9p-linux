#!/bin/bash -ex
#/busybox-i486 mkdir -p bin usr/sbin usr/bin sbin
#/busybox-i486 --install -s
/busybox-i486 mount -a
mount --move sys host9p/fsroot/sys
mount --move proc host9p/fsroot/proc
mkdir host9p/fsroot/dev
mount --move dev host9p/fsroot/dev
mount --bind host9p/fsroot host9p/fsroot
exec /busybox-i486 switch_root host9p/fsroot /init
